{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "globals": {
    "__DEV__": false,
    "cssInJS": false,
    "jest": false,
    "describe": false,
    "it": false,
    "expect": false
  },
  "rules": {
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "react/react-in-jsx-scope": 0
  }
}
